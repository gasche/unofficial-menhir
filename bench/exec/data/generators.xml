<generators>
  <generatorgroup>
    <generator>
      <title>Menhir - Code</title>
      <path>../../src</path>
      <command>menhir.opt</command>
      <traceflag>--trace</traceflag>
      <parsersuffix>code</parsersuffix>
      <libraries/>
    </generator>
    <generator>
      <title>Menhir - Tables</title>
      <path>../../src/</path>
      <command>menhir.opt --table</command>
      <traceflag>--trace</traceflag>
      <parsersuffix>table</parsersuffix>
      <libraries>
        <library>menhirLib</library>
      </libraries>
    </generator>
  </generatorgroup>
  <generator>
    <title>OCamlYacc</title>
    <path></path>
    <command>ocamlyacc</command>
    <traceflag></traceflag>
    <traceenv OCAMLRUNPARAM="p"/>
    <parsersuffix>yacc</parsersuffix>
    <libraries/>
  </generator>
</generators>